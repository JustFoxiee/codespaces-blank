local Rayfield
task.spawn(function()
    Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
end)

repeat task.wait() until Rayfield  -- Wait for Rayfield to load

local Window = Rayfield:CreateWindow({
   Name = "VLK Hub",
   Icon = 0, 
   LoadingTitle = "VLK Scripts",
   LoadingSubtitle = "by Velkzie",
   Theme = "Amethyst",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = false,
      FolderName = nil,
      FileName = "VLK Hub"
   },
})

local MainTab = Window:CreateTab("üè†Home", nil)
local MainSection = MainTab:CreateSection("Main")

Rayfield:Notify({
   Title = "You executed the script!",
   Content = "Pookies Gui",
   Duration = 3,
   Image = nil,
})

local infYieldScript = game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source')
local feAnimationsScript = game:HttpGet("https://raw.githubusercontent.com/ExploitFin/AquaMatrix/refs/heads/AquaMatrix/AquaMatrix")

local Button = MainTab:CreateButton({
   Name = "Inf Yield",
   Callback = function()
      loadstring(infYieldScript)()
   end,
})

local Button = MainTab:CreateButton({
   Name = "FE Animations",
   Callback = function()
      loadstring(feAnimationsScript)()
   end,
})

local TrollTab = Window:CreateTab("TrollFeaturesü§°", nil)
local MainSection = TrollTab:CreateSection("Trollü§°")

local Button = TrollTab:CreateButton({
   Name = "Jerk Off R15",
   Callback = function()
   loadstring(game:HttpGet("https://pastefy.app/YZoglOyJ/raw"))()
   end,
})

local Button = TrollTab:CreateButton({
   Name = "Jerk Off R6",
   Callback = function()
   loadstring(game:HttpGet("https://pastefy.app/wa3v2Vgm/raw"))()
   end,
})